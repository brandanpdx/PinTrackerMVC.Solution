@{
	ViewData["Title"] = "Home Page";
}
@using PinTrackerMVC.Models

<div class="card">
    <img class="img-fluid card-img" src="https://i.imgur.com/8ucZAoy.png">
    <div class="card-img-overlay d-flex">
      <div class="my-auto mx-auto text-center">
				<div class="card-title">
					<p class="text-light">Welcome to the Portland Pinball Tracker!</p>
					<p class="text-light">Your Portland Pinball Database</p>
				</div>
      </div>
    </div>
  </div>

<div class="card">
	<div id="index-section">
			<div class="text-center">
					<h1 class="display-4">Pinball Machine Search</h1>

					@using (Html.BeginForm("Search", "Locations"))
					{
							<select class="form-control" id="SearchBy" name="SearchBy">
							<option>Search by machine</option> 
							<option>Search by location</option> 
							</select>
							<input type="text" placeholder="Search.." name="search" class="form-control"/>
							<br>
							<input type="submit"  value="Search!" class="btn btn-dark btn-lg">
					}   
			</div> 
	</div>


